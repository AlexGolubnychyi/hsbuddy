// Place your settings in this file to overwrite default and user settings.
{
   "files.exclude": {
		"**/.git": true,
        "**/.gitignore":true,
		"**/bin":false,
        "**/obj":true,
        "**/Publish":true,
        "**/node_modules":true,
        "**/*.js": { "when": "$(basename).ts"},
        "**/*.map": true,
        "**/*.css": { "when": "$(basename).less"}
	},
    "editor.snippetSuggestions": "bottom"
}
