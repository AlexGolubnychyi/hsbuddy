{
  "name": "hearth.stone.db",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "6.2.2",
    "npm": "3.9.5"
  },
  "scripts": {
    "start": "node ./server/bin/index.js",
    "tsc": "node ./node_modules/typescript/bin/tsc",
    "tscc": "node ./node_modules/typescript/bin/tsc -p tsconfig-client.json",
    "ngc": "node node_modules/@angular/compiler-cli/src/main.js -p ngc.json",
    "migrate": "node ./server/bin/migration.js",
    "bundle": "node ./node_modules/browserify/bin/cmd.js -s main client/main.js > public/app.js ",
    "minify":"node ./node_modules/uglifyjs/bin/uglifyjs public/app.js --screw-ie8 --compress --output public/app.min.js",
    "build2": "npm run tsc && npm run bundle && npm run minify",
    "build": "npm run tsc && node ./server/bin/build && npm run minify",
    "postinstall": "node run build && npm run migrate"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "^0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "^3.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.5",
    "@angular/compiler-cli": "0.5.0",
    "@angular/platform-server": "2.0.0-rc.5",
    "bluebird": "*",
    "browserify": "*",
    "uglifyjs": "*",
    "body-parser": "~1.15.1",
    "bootstrap": "^3.3.6",
    "cheerio": "*",
    "connect-mongo": "^1.3.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.0",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-session": "^1.14.0",
    "jade": "~1.11.0",
    "less-middleware": "^2.2.0",
    "mongodb": "^2.2.4",
    "mongoose": "^4.5.7",
    "morgan": "~1.7.0",
    "ng2-bootstrap": "^1.0.24",
    "reflect-metadata": "^0.1.3",
    "request": "*",
    "rxjs": "5.0.0-beta.6",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.27",
    "systemjs-builder": "^0.15.26",
    "typescript": "^1.9.0-dev",
    "zone.js": "^0.6.12"
  }
}
