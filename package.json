{
  "name": "hearth.stone.db",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "6.2.2",
    "npm": "3.9.5"
  },
  "scripts": {
    "start": "node ./server/bin/index.js",
    "tsci": "tsc -p tsconfig-int.json",
    "ngc": "ngc -p ngc.json",
    "rollup": "rollup -f iife -c -o public/temp/app.es2015.js",
    "convert-to-es5": "tsc --allowJs public/temp/app.es2015.js --out public/temp/app.js --lib es5,es6,dom",
    "migrate": "node ./server/bin/migration.js",
    "minify": "uglifyjs public/temp/app.js --screw-ie8 --compress --mangle --output public/app.min.js",
    "clean": "del-cli public/temp",
    "build": "del-cli ngfactory && npm run ngc && npm run tsci && npm run rollup && npm run convert-to-es5 && npm run minify && tsc && npm run clean",
    "postinstall": "npm run build && npm run migrate",
    "_bundle": "browserify -s main client/main.js > public/app.js ",
    "_build": "tsc && node ./server/bin/build && npm run minify",
    "_build1": "tsc && npm run _bundle && npm run minify"
  },
  "dependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/compiler-cli": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/forms": "2.1.0",
    "@angular/http": "2.1.0",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "@angular/platform-server": "2.1.0",
    "@angular/router": "3.1.0",
    "@types/bluebird": "^3.0.35",
    "@types/body-parser": "0.0.33",
    "@types/cheerio": "^0.17.31",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.33",
    "@types/express-jwt": "0.0.33",
    "@types/express-serve-static-core": "^4.0.35",
    "@types/express-session": "0.0.32",
    "@types/jsonwebtoken": "^7.1.33",
    "@types/mongodb": "^2.1.33",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.45",
    "@types/request": "0.0.32",
    "angular2-jwt": "^0.1.25",
    "bluebird": "*",
    "body-parser": "~1.15.1",
    "bootstrap": "^3.3.6",
    "browserify": "*",
    "cheerio": "*",
    "compression": "1.6.2",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "del-cli": "0.2.0",
    "express": "~4.13.4",
    "express-jwt": "^5.1.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^7.1.9",
    "less-middleware": "^2.2.0",
    "mongodb": "^2.2.4",
    "mongoose": "^4.5.7",
    "morgan": "~1.7.0",
    "ng2-bootstrap": "^1.1.14",
    "reflect-metadata": "^0.1.3",
    "request": "*",
    "rollup": "0.34.13",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "2.0.0",
    "rxjs": "5.0.0-rc.1",
    "rxjs-es": "5.0.0-beta.12",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.27",
    "typescript": "2.0.3",
    "uglifyjs": "*",
    "zone.js": "^0.6.25"
  }
}
