{
  "name": "hearth.stone.db",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "10.9.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "node ./dist/server/bin/index.js",
    "init": "concurrently \"npm run server-watch\" \"npm run server-run\" \"npm run client-serve\"",
    "postinstall": "npm run build && npm run migrate-db",
    "deploy": "git push heroku master",
    "server-watch": "tsc -p tsconfig.server.json -w",
    "server-run": "tsc -p tsconfig.server.json & nodemon  --inspect dist/server/bin/index.js --watch dist/server",
    "server-serve": "concurrently \"npm run server-watch\" \"npm run server-run\"",
    "client-serve": "ng serve --aot --proxy-config proxy.conf.json",
    "build": "npm run client-build & npm run server-build",
    "client-build": "ng build --aot --prod",
    "server-build": "tsc -p tsconfig.server.json",
    "migrate-db": "node ./dist/server/bin/migration.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/cli": "6.1.5",
    "@angular/common": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/router": "^6.1.6",
    "@auth0/angular-jwt": "2.0.0",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.3.7",
    "chalk": "^1.1.3",
    "cheerio": "1.0.0-rc.2",
    "compression": "^1.7.3",
    "core-js": "^2.5.3",
    "debug": "^3.1.0",
    "del-cli": "^0.2.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.3.0",
    "mongodb": "^3.1.4",
    "mongoose": "^5.2.13",
    "morgan": "^1.9.0",
    "ngx-bootstrap": "3.0.1",
    "request": "2.83.0",
    "rxjs": "^6.3.2",
    "valid-url": "^1.0.9",
    "zone.js": "^0.8.26"
  },
