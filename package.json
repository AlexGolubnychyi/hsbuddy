{
  "name": "hearth.stone.db",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "9.2.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "node ./dist/server/bin/index.js",
    "server-build": "tsc -p tsconfig.server.json",
    "server-watch": "tsc -p tsconfig.server.json -w",
    "server-run": "tsc -p tsconfig.server.json & nodemon  --inspect dist/server/bin/index.js --watch dist/server",
    "server-serve": "concurrently \"npm run server-watch\" \"npm run server-run\"",
    "client-serve": "ng serve --aot --proxy-config proxy.conf.json",
    "client-build-dev": "ng build",
    "client-build": "ng build --aot --prod",
    "init": "concurrently \"npm run server-watch\" \"npm run server-run\" \"npm run client-serve\"",
    "build": "npm run client-build & npm run server-build",
    "easy-client-serve": "ng build --watch",
    "easy-serve": "concurrently \"npm run server-serve\" \"npm run easy-client-serve\"",
    "easy-run": "concurrently \"npm run server-run\" \"npm run client-build-dev\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "git push heroku master",
    "migrate-db": "node ./dist/server/bin/migration.js",
    "postinstall": "npm run build && npm run migrate-db"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/animations": "^5.1.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@auth0/angular-jwt": "1.0.0-beta.9",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19",
    "bluebird": "3.5.0",
    "body-parser": "1.15.2",
    "bootstrap": "^3.3.7",
    "chalk": "^1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.7.1",
    "debug": "~2.3.0",
    "del-cli": "^0.2.0",
    "express": "~4.14.0",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.1.9",
    "mongodb": "3.0.1",
    "mongoose": "4.6.6",
    "morgan": "~1.7.0",
    "ngx-bootstrap": "2.0.0-rc.0",
    "request": "2.83.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@angular/language-service": "^5.1.2",
    "@types/bluebird": "3.5.2",
    "@types/body-parser": "0.0.33",
    "@types/chalk": "^0.4.31",
    "@types/cheerio": "^0.17.31",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.33",
    "@types/express-jwt": "0.0.33",
    "@types/express-serve-static-core": "^4.0.35",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/jsonwebtoken": "^7.1.33",
    "@types/less-middleware": "^2.0.29",
    "@types/mongodb": "^2.1.34",
    "@types/morgan": "^1.7.32",
    "@types/node": "~6.0.60",
    "@types/request": "2.0.9",
    "@types/serve-favicon": "^2.2.28",
    "@types/valid-url": "^1.0.2",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.5.3"
  }
}
